#include <stdio.h> 
#include <math.h>

int main ()
{
	double x,y,xt;
	printf("Enter x = ",&x);
	scanf("%lf",&x);
	y = sin(x) + cos(x); 
	xt = pow(x,2); // to make it easier to read
	y = y / log(xt+exp(-1));
	if (y<0) 
	{
		printf("Error");
		return 1;
	}
	y = sqrt(y);
	printf("Q=%1.2lf",y);
	return 0;
}
