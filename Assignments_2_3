#include <stdio.h>

int main ()
{
	float x,y;
	printf("Enter x : ");
	scanf("%f",&x);
	printf("Enter y : ");
	scanf("%f",&y);
	if (x>0)
	{
		if (y>0) printf("Point is in : I");
		else if (y==0) printf("Point is on Y axis");
		else if (y<0) printf("Point is in : IV");
	}
	else if (x<0)
	{
		if (y>0) printf("Point is in : II");
		else if (y==0) printf("Point is on Y axis");
		else if (y<0) printf("Point is in : III");
	}
	else if (x==0)
	{
		if (y>0) printf("Point is on X axis");
		else if (y==0) printf("Point is in centre of the field");
		else if (y<0) printf("Point is on X axis");
	}
	return 0;
}
